
fairseq-preprocess \
    --source-lang de --target-lang en \
    --trainpref ../custom_data_nmt/fastBPE/EMEA/train \
    --validpref ../custom_data_nmt/fastBPE/EMEA/valid \
    --testpref ../custom_data_nmt/fastBPE/EMEA/test \
    --destdir ../data-bin/custom_EMEA/orig_EMEA --workers 20 \
    --joined-dictionary --srcdict ../models/wmt19.de-en.joined-dict.ensemble/dict.de.txt


